{
    "env": {
        "es6": false,
        "qunit": true
    },
    "globals": {
        "jarallax": true,
        "YT": true,
        "Vimeo": true,
        "UTILS": true
    },
    "rules": {
        "no-var": 0,
        "vars-on-top": "off",
        "object-shorthand": "off",
        "prefer-arrow-callback": "off",
        "prefer-template": "off",
        "prefer-rest-params": "off"
    }
}
